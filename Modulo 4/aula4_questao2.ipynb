    "# Leitura do valor de n\n",
    "n = int(input(\"Digite um número: \"))\n",
    "\n",
    "# Inicialização do contador\n",
    "cont = 0\n",
    "\n",
    "# Estrutura de repetição\n",
    "while cont < n:\n",
    "    cont += 1\n",
    "    print(cont)\n",
    "\n",
    "# Impressão final\n",
    "print(\"Fim\")\n"
   ]
  }
