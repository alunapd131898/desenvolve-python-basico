    "import random\n",
    "import math\n",
    "\n",
    "n = int(input(\"Digite a quantidade de números aleatórios: \"))\n",
    "numeros = [random.randint(0, 100) for _ in range(n)]\n",
    "raiz_quadrada = math.sqrt(sum(numeros))\n",
    "\n",
    "print(f\"Raiz quadrada da soma: {round(raiz_quadrada, 2)}\")\n"
   ]
  }
